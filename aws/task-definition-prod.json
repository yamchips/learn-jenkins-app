{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learn-jenkins-app-task-definition-prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp-prod",
            "image": "IMAGE_URI",
            "portMappings": [
                {
                    "name": "nginx-80-tip",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::481961344643:role/ecsTaskExecutionRole"
}